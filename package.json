{
    "name": "react-starter-boilerplate",
    "version": "1.0.0",
    "description": "React starter boilerplate",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --env.REACT_APP_ENV=DEV --config=config/webpack.dev.js --hot",
        "start:reload": "webpack-dev-server --config=config/webpack.dev.js",
        "start:style": "cross-env MAP_STYLE=true webpack-dev-server --config=config/webpack.dev.js --hot",
        "build": "webpack --env.REACT_APP_ENV=PROD --config=config/webpack.prod.js",
        "analyze": "cross-env ENABLE_ANALYZER=true npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "React",
        "boilerplate",
        "React",
        "starter",
        "kit"
    ],
    "author": "onejeet",
    "license": "MIT",
    "dependencies": {
        "autoprefixer": "^9.8.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-manifest-plugin": "^2.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.3",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-merge": "^4.2.2"
    },
    "browserslist": [
        "defaults",
        "not ie < 11",
        "last 2 versions",
        "> 1%",
        "iOS 7",
        "last 3 iOS versions"
    ]
}
